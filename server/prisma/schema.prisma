generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Book {
  id         String   @id @default(uuid())
  title      String
  pages      Int 
  autor      String
  created_at DateTime

  page  Pages   @relation(fields: [pages], references: [id])

  @@map("books")
}

model Notes {
  id         String   @id @default(uuid())
  autor      String
  created_at DateTime

  page       Pages[] 

  @@map("notes")

}

